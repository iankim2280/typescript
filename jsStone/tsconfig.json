{
  "compilerOptions": {
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "strictNullChecks": true,
    "strict": true,
    "lib": ["DOM", "ES6", "ES2016", "ES2017", "ES2018", "ES2019", "ES2020"],
    "target": "ES6",
    "typeRoots": ["./types", "./node_modules/@types"]
  },
  "exclude": ["*.js"]
}
